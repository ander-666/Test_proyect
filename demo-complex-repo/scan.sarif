{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "secrets-llm-scanner",
          "informationUri": "https://example.local",
          "version": "0.1.0",
          "rules": [
            {
              "id": "SECRET.LLM",
              "name": "LLM-validated secret",
              "shortDescription": {
                "text": "Potential secret"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "SECRET.LLM",
          "level": "error",
          "message": {
            "text": "API Key: Found in 'hashes' array, a common storage for AWS credentials"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "data/sample.json"
                },
                "region": {
                  "startLine": 14
                }
              }
            }
          ],
          "properties": {
            "match": "4b825dc642cb6eb9a060e54bf8d69288fbee4904",
            "pattern_id": "aws_secret_access_key",
            "confidence": 0.8
          }
        },
        {
          "ruleId": "SECRET.LLM",
          "level": "error",
          "message": {
            "text": "Private Key: matches private key block with strong encryption"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "certs/private_key.pem"
                },
                "region": {
